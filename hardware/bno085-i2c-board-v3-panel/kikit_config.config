{
	"layout": {
		"type": "grid",
		"rows": 5,
		"cols": 5,
		"space": "2mm",
		"rotation": "0deg",
		"alternation": "none",
		"renamenet": "Board_{n}-{orig}",
		"renameref": "{orig}"
	}, 
	"source": {
		"type": "auto",
		"tolerance": "1mm"
	},
	"tabs": {
		"type": "annotation",
	},
	"cuts": {
		"type": "mousebites",
		"offset": "0mm",
		"drill": "0.5mm",
		"spacing": "1mm",
	},
	"framing": {
		"type": "frame",
		"thickness": "2mm",
		"cuts" : "both",
		"space": "2mm"
	},
	"text": {
		"text": "{boardTitle} | {boardDate}"
	},
	"post": {
		"type": "auto",
		"millradius": "1mm",
		"copperfill": false,
		"dimensions": true
	}
}
